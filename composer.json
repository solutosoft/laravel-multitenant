{
  "name": "solutosoft/laravel-multitenant",
  "description": "The multi tenant control with shared table",
  "keywords": [
    "laravel",
    "multi-tenant",
    "eloquent",
    "database"
  ],
  "homepage": "http://slto.com.br",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://gitlab.com/solutosoft/laravel-multitenant/issues",
    "source": "https://gitlab.com/solutosoft/laravel-multitenant"
  },
  "authors": [
    {
      "name": "Leandro Guindani Gehlen",
      "email": "leandrogehlen@gmail.com"
    }
  ],
  "require": {
    "illuminate/auth": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
    "illuminate/database": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
    "illuminate/events": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "Solutosoft\\MultiTenant\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Solutosoft\\MultiTenant\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
